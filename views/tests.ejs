<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <p>From <%= name %></p>
    <p>Animal <%= animals[1] %></p>
    <% animals.forEach(animal => { %>
      <%= animal %><br/>
    <% }) %>
    <p>Ciudades:</p>
    <% ciudades.forEach(ciudad => { %>
      <%= ciudad.id %> - <%= ciudad.nombre %><br/>
    <% }) %>
  </body>
</html>
